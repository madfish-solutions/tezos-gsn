{
  "name": "tezos-gsn",
  "private": true,
  "config": {
    "network": "http://127.0.0.1:8732"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development ts-node-dev --files --quiet src/index.ts",
    "start-prod": "cross-env NODE_ENV=production node dist/index.js",
    "_prebuild": "run-s lint clean",
    "build": "tsc --skipLibCheck",
    "lint": "eslint --ext ts,js,mjs,json src",
    "clean": "rimraf dist/",
    "deploy": "node scripts/cli.js deploy -p $npm_package_config_network stablecoin",
    "db:setup": "knex migrate:latest; knex seed:run",
    "db:drop": "knex migrate:rollback",
    "db:reset": "npm run db:drop; npm run db:setup",
    "db:mm": "knex migrate:make",
    "db:sm": "knex seed:make",
    "docker:pgdev-init": "docker run -d --name tgsn_db_dev -v tgsn_dev_dbdata:/var/lib/postgresql/data -p 5432:5432 -e POSTGRES_DB=tgsn_dev -e POSTGRES_PASSWORD=postgres postgres:alpine",
    "docker:pgdev-start": "docker start tgsn_db_dev",
    "docker:pgdev-stop": "docker stop tgsn_db_dev",
    "docker:pgdev-rm": "docker stop tgsn_db_dev; docker rm tgsn_db_dev; docker volume rm tgsn_dev_dbdata"
  },
  "dependencies": {
    "@taquito/signer": "^8.1.0",
    "@taquito/taquito": "^8.1.0",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.1",
    "commander": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.9.2",
    "helmet": "^4.4.1",
    "http-errors": "^1.8.0",
    "knex": "^0.21.17",
    "knex-stringcase": "^1.4.5",
    "memoizee": "^0.4.15",
    "ms": "^2.1.3",
    "nanoid": "^3.1.20",
    "pg": "^8.5.1",
    "pino": "^6.11.0",
    "pino-http": "^5.5.0",
    "pino-pretty": "^4.5.0",
    "sqlite3": "^5.0.2",
    "threads": "^1.6.3"
  },
  "devDependencies": {
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.11",
    "@types/http-errors": "^1.8.0",
    "@types/memoizee": "^0.4.5",
    "@types/ms": "^0.7.31",
    "@types/node": "^14.14.25",
    "@types/pino": "^6.3.5",
    "@types/pino-http": "^5.4.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.19.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.1",
    "typechain": "^4.0.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">= 12.10"
  }
}
